# EditorConfig is awesome: https://EditorConfig.org

# すべてのファイルに適用
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

# TypeScript / JavaScript
[*.{ts,js,json,md}]
indent_size = 2

# CSV ファイルはスペースではなくタブにする場合など指定可能
[*.csv]
indent_style = comma